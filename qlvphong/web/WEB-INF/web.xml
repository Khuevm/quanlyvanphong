<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AdminServlet</servlet-name>
        <servlet-class>qlvphong.servlet.AdminServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NhanVienServlet</servlet-name>
        <servlet-class>qlvphong.servlet.NhanVienServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KhachHangServlet</servlet-name>
        <servlet-class>qlvphong.servlet.KhachHangServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ToaNhaServlet</servlet-name>
        <servlet-class>qlvphong.servlet.ToaNhaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HopDongServlet</servlet-name>
        <servlet-class>qlvphong.servlet.HopDongServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HoaDonServlet</servlet-name>
        <servlet-class>qlvphong.servlet.HoaDonServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminServlet</servlet-name>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NhanVienServlet</servlet-name>
        <url-pattern>/nhanvien</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KhachHangServlet</servlet-name>
        <url-pattern>/khachhang</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ToaNhaServlet</servlet-name>
        <url-pattern>/toanha</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HopDongServlet</servlet-name>
        <url-pattern>/hopdong</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HoaDonServlet</servlet-name>
        <url-pattern>/hoadon</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>dangnhap.jsp</welcome-file>
    </welcome-file-list>
</web-app>
